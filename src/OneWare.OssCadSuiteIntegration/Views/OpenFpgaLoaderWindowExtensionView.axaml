<UserControl xmlns="https://github.com/avaloniaui"
             xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
             xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
             xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
             xmlns:viewModels="clr-namespace:OneWare.OssCadSuiteIntegration.ViewModels"
             mc:Ignorable="d" d:DesignWidth="800" d:DesignHeight="450" IsVisible="{Binding IsVisible}"
             x:Class="OneWare.OssCadSuiteIntegration.Views.OpenFpgaLoaderWindowExtensionView"
             x:DataType="viewModels:OpenFpgaLoaderWindowExtensionViewModel">
    <MenuItem Header="OpenFPGALoader Settings"
              Command="{Binding OpenSettingsAsync}"
              IsEnabled="{Binding IsEnabled}">
        <MenuItem.Icon>
            <Image Source="{DynamicResource Material.SettingsOutline}" />
        </MenuItem.Icon>
    </MenuItem>
</UserControl>