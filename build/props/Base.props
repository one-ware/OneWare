<?xml version="1.0" encoding="utf-8"?>
<Project DefaultTargets="Build" xmlns="http://schemas.microsoft.com/developer/msbuild/2003">
    <PropertyGroup>
        <StudioVersion>1.0.4.3</StudioVersion>
        <CoreVersion>$(StudioVersion)</CoreVersion>
        <UniversalFpgaProjectSystemVersion>$(StudioVersion)</UniversalFpgaProjectSystemVersion>
        <EssentialsVersion>$(StudioVersion)</EssentialsVersion>
        <Version>$(StudioVersion)</Version>
        <Authors>Hendrik Mennen</Authors>
        <Description>Next Generation IDE for Electronics Development</Description>
        <PackageLicenseExpression>MIT</PackageLicenseExpression>
        <PackageIcon>package-icon.png</PackageIcon>
        <PackageTags>oneware;oneware-studio</PackageTags>
    </PropertyGroup>

    <ItemGroup>
        <None Include="./../../package-icon.png" Pack="true" PackagePath="/" />
    </ItemGroup>
</Project>
